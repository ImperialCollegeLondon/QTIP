//function to send (i2cnumber) multiple bytes starting at i2cRA
void i2cmultiSB () {
  i2cstart ();
  i2cbyte = 0xD0;              //device address + write
  i2cSB ();
  i2cbyte = i2cRA;             //register address
  i2cSB ();
  //sending bits now and receiving ack/nack
  for (i2ccount = 0; i2ccount < i2cnumber; i2ccount++) {
    i2cbyte = i2coutchar[i2ccount]; //copy from buffer
    i2cSB ();                  //send i2cbyte and ignore ackbit
  }                            //next
  i2cstop ();
}                              //end of i2cmultiSB ()

//function to read (i2cnumber) multiple bytes starting at i2cRA
void i2cmultiRB () {
  i2cstart ();
  i2cbyte = 0xD0;              //device address + write
  i2cSB ();
  Serial.println("Flag");
  i2cbyte = i2cRA;             //register address
  i2cSB ();                    //SDA should end tristate for ack/nak
  DDRB = DDRB & B11101111;     //SCL 0->1 ready for restart
  i2cdly ();
  i2cstart ();                 //restart to read from chosen register
  i2cbyte = 0xD1;              //device address + read
  i2cSB ();
  //reading bits now and sending back ack/nack
  for (i2ccount = 0; i2ccount < i2cnumber; i2ccount++) {
    i2cRB ();                  //read char to i2cbyte
    i2cinchar[i2ccount] = i2cbyte; //copy to buffer
    if (i2ccount == i2cnumber - 1)
      i2cS1 ();                //send nack on last char
    else
      i2cS0 ();                //send ack
  }                            //next
  i2cstop ();
}                              //end of i2cmultiRB ()

//function to send bit 0. enter & exit clock low
void i2cS0 () {
  i2cdly ();
  DDRB = DDRB | B00001000;     //SDA =0
  i2cdly ();
  DDRB = DDRB & B11101111;     //SCL 0->1
  while (!(PINB & B00010000)); //test for clock stretching
  i2cdly ();
  DDRB = DDRB | B00010000;     //SCL=0
}

//function to send bit 1. enter & exit clock low
void i2cS1 () {
  i2cdly ();
  DDRB = DDRB & B11110111;     //SDA=1
  i2cdly ();
  DDRB = DDRB & B11101111;     //SCL 0->1
  while (!(PINB & B00010000)); //test for clock stretching
  i2cdly ();
  DDRB = DDRB | B00010000;     //SCL=0
}

//function to set start condition from bus released
void i2cstart () {
  DDRB = DDRB | B00001000;     //SDA 1->0 signal start
  i2cdly ();
  DDRB = DDRB | B00010000;     //SCL 1->0
  i2cdly ();
}

//function to set stop condition enter clock low
void i2cstop () {
  DDRB = DDRB | B00001000;     //SDA=0
  i2cdly ();
  DDRB = DDRB & B11101111;     //SCL=1
  i2cdly ();
  DDRB = DDRB & B11110111;     //SDA 0->1 signal stop
  i2cdly ();
}

//function to send byte held in i2cbyte
//enter & exit clock low, exit tristate SDA
void i2cSB () {
  for (int i = 0; i < 8; i++) {
    i2cdly ();
    Serial.println("");
    Serial.println("Flag");
    if (i2cbyte < 0x80)
      DDRB = DDRB | B00001000; //SDA=0
    else
      DDRB = DDRB & B11110111; //SDA=1
    i2cdly ();
    Serial.println("Flag");
    DDRB = DDRB & B11101111;   //SCL=0->1 to clock data
    i2cbyte = i2cbyte * 2;     //shift byte left
    while (!(PINB & B00010000)); //test for clock stretching
    DDRB = DDRB | B00010000;   //SCL=0, data changes when low
  }                            //next
  i2cdly ();                   //now clock ack bit in
  DDRB = DDRB & B11110111;     //Tri-State SDA
  i2cdly ();
  DDRB = DDRB & B11101111;     //SCL 0->1 to clock ack
  while (!(PINB & B00010000));   //test for clock stretching
  i2cackbit = (PINB & B00001000); //read ack bit but ignore
  i2cdly ();
  DDRB = DDRB | B00010000;     //SCL=0 so slave can release SDA
}                              //end of i2cSB ()

//function to receive byte into i2cbyte
//enter & exit with clock low, exit tristate SDA
void i2cRB () {
  DDRB = DDRB & B11110111;     //Tri-State SDA
  i2cbyte = 0;
  for (int i = 0; i < 8; i++) {
    i2cdly ();                 //clock byte in
    DDRB = DDRB & B11101111;   //SCL 0->1 to clock data in
    while (!(PINB & B00010000)); //test for clock stretching
    i2cbyte = i2cbyte * 2;     //shift byte left
    if (PINB & B00001000)      //read bit
      i2cbyte++;               //set LSB to 1
    i2cdly ();
    DDRB = DDRB | B00010000;   //SCL=0, data changes when low
  }                            //next
}                              //end of i2cRB ()




